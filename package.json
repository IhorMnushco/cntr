{
  "name": "@koterion/country_list",
  "version": "1.2.0",
  "description": "Plugin for creale list of country",
  "scripts": {
    "dev": "webpack --mode development",
    "build-only": "webpack --mode production",
    "prod": "del-cli dist && webpack --mode production && html dist/*.html --indent-size 2",
    "watch": "webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --open",
    "beautify": "html dist/*.html --indent-size 2",
    "clear": "del-cli dist"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "ie >= 10",
    "Firefox ESR",
    "Safari >= 10"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true,
      "jquery": true,
      "es6": true
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/countryList.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/koterion/country_list"
  },
  "author": "koterion",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.2",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.3",
    "del-cli": "^1.1.0",
    "html-cli": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
